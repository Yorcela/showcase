{
  "name": "yorcela-showcase",
  "version": "1.0.0",
  "description": "Yorcela Showcase",
  "author": "Alec ROY",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "// ðŸ§¹  CLEAN / RESET ENVIRONNEMENT": "--------------------------",
    "apps:recreate": "pnpm apps:clean && pnpm apps:init",
    "apps:clean": "bash ./tools/scripts/docker-clean.sh",
    "apps:init": "pnpm dotenv:init && pnpm install && pnpm apps:start && pnpm db:fresh",
    "apps:start": "sh -lc 'pnpm dc ${NODE_ENV:-development} up'",
    "apps:close": "sh -lc 'pnpm dc ${NODE_ENV:-development} down'",
    "apps:restart": "sh -lc 'pnpm dc ${NODE_ENV:-development} restart'",
    "backend:restart": "pnpm apps:down && docker compose build backend && docker compose up -d backend",
    "// ðŸ³ DOCKER - DEV": "--------------------------",
    "dc": "node tools/scripts/docker-short.mjs",
    "dev:up": "pnpm dc development up",
    "dev:down": "pnpm dc development down",
    "dev:restart": "pnpm dc development restart",
    "// ðŸ³ DOCKER - PROD": "--------------------------",
    "prod:up": "pnpm dc production up",
    "prod:down": "pnpm dc production down",
    "prod:restart": "pnpm dc production restart",
    "// ðŸ—ï¸ LOCAL (hors Docker)": "--------------------------",
    "dev:local:init": "pnpm db:fresh && pnpm dev:local",
    "dev:local": "pnpm dev && pnpm prisma:studio",
    "dev": "nest start --watch",
    "start": "node dist/main.js",
    "prod:build": "cross-env NODE_ENV=production nest build",
    "prod:start": "pnpm prod:build && cross-env NODE_ENV=production pnpm start",
    "// ðŸ§ª  TESTS": "--------------------------",
    "test": "cross-env NODE_ENV=test jest --runInBand --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --runInBand --no-cache --coverage",
    "test:ci": "pnpm typecheck && pnpm lint && pnpm format:check && pnpm test:cov",
    "// ðŸ—ï¸ CI": "--------------------------",
    "ci:init": "pnpm install --frozen-lockfile && pnpm dotenv:init && pnpm db:fresh",
    "ci:test": "cross-env NODE_ENV=ci jest --runInBand --no-cache --coverage",
    "// ðŸ§­  DATABASE (using Prisma ORM)": "--------------------------",
    "db:fresh": "pnpm db:generate && pnpm db:update",
    "db:reset": "pnpm prisma:reset",
    "db:update": "pnpm prisma:update",
    "db:sync": "pnpm prisma:sync",
    "db:generate": "pnpm prisma:generate",
    "// ðŸ§­  PRISMA (tout passe par prisma-run.sh)": "--------------------------",
    "prisma:generate": "tools/scripts/prisma-run.sh generate",
    "prisma:update:dev": "pnpm prisma:schemas:sync && tools/scripts/prisma-run.sh migrate dev",
    "prisma:update:prod": "tools/scripts/prisma-run.sh migrate deploy",
    "prisma:update": "bash -lc 'if [ \"${NODE_ENV:-development}\" = \"production\" ]; then pnpm prisma:update:prod; else pnpm prisma:update:dev; fi'",
    "prisma:sync": "tools/scripts/prisma-run.sh db push",
    "prisma:reset": "tools/scripts/prisma-run.sh migrate reset -f",
    "prisma:studio": "tools/scripts/prisma-run.sh studio --port ${PRISMA_STUDIO_PORT:-5555} --browser none",
    "prisma:schemas:sync": "node tools/scripts/prisma-sync-schemas.mjs write",
    "// ðŸ“˜  DOCS / OPENAPI": "--------------------------",
    "doc:gen": "pnpm db:generate && ts-node -r tsconfig-paths/register -r dotenv-flow/config tools/scripts/openapi-generate.ts",
    "doc:typedoc": "typedoc --out docs/api src",
    "doc:serve": "http-server docs -p 8080",
    "// ðŸ§°  QUALITÃ‰ / LINT / FORMAT": "--------------------------",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "pnpm format && pnpm lint --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md,yml,yaml}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check:all": "pnpm typecheck && pnpm lint && pnpm format:check && pnpm test",
    "// ðŸ”  HUSKY / DOTENV": "--------------------------",
    "prepare": "husky",
    "dotenv:init": "bash ./tools/scripts/dotenv-init.sh",
    "dotenv:check": "node tools/scripts/dotenv-check.mjs",
    "dotenv:print": "sh -lc 'printenv | sort'",
    "dotenv:fix": "pnpm dotenv:check --fix",
    "imports:fix": "pnpm ts-node tools/scripts/ts-barrel-to-relative.ts",
    "precommit": "pnpm imports:fix && pnpm dotenv:fix && pnpm prisma:schemas:sync && pnpm lint:fix && pnpm lint && pnpm test:cov && git add . && git status"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.7",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.7",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.7",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@paralleldrive/cuid2": "^3.1.0",
    "@prisma/client": "^6.18.0",
    "@scalar/nestjs-api-reference": "^1.0.5",
    "argon2": "^0.44.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv-flow": "^4.1.0",
    "express": "^4.19.2",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.7",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^5.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "24.9.1",
    "@types/nodemailer": "^7.0.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "gitbook-cli": "^2.3.2",
    "globals": "^16.4.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "i": "^0.3.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "pnpm": "^10.19.0",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-morph": "^27.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.14",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
